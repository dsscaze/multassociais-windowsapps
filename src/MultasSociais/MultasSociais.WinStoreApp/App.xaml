<caliburn:CaliburnApplication
    x:Class="MultasSociais.WinStoreApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:caliburn="using:Caliburn.Micro"
    RequestedTheme="Light"
    xmlns:local="using:MultasSociais.WinStoreApp"
    xmlns:localData="using:MultasSociais.WinStoreApp.DataModel">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Common/ExtraStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <!-- Application-specific resources -->
            <x:String x:Key="AppName">Multas Sociais</x:String>
        </ResourceDictionary>
    </Application.Resources>
</caliburn:CaliburnApplication>